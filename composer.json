  "autoload-dev": {
    "psr-4": {
      "PVerdu\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "phpseclib/phpseclib": "^2.0",
    "illuminate/contracts": "^6.0",
    "symfony/http-kernel": "^4.3.4",
    "symfony/finder": "^4.3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mikey179/vfsstream": "^1.6",
    "laravel/framework": "^6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PVerdu\\x509CertificateValidator\\x509CertificateValidatorServiceProvider"
      ]
    }
  }
}
