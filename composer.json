{
  "name": "pverdu/x509-certificate-validator",
  "description": "A X509 Certificate validator",
  "authors": [
    {
      "name": "P. Verdu",
      "email": "paulverdu20@gmail.com"
    },
    {
      "name": "Sven E. Stolk",
      "email": "sestolk91@gmail.com"
    }
  ],
  "keywords": [
    "library",
    "x509",
    "certificate",
    "validation",
    "laravel"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "PVerdu\\x509CertificateValidator\\": "src/"
    }
